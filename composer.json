{
    "name": "dazzle-php/ssh",
    "description": "Dazzle Asynchronous SSH.",
    "keywords": [
        "dazzle", "dazzle-php", "async", "asynchronous", "ssh", "ssh2", "driver"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/ssh/issues",
        "source": "https://github.com/dazzle-php/ssh"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/ssh/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7",
        "dazzle-php/event": "0.5.*",
        "dazzle-php/loop": "0.5.*",
        "dazzle-php/stream": "0.5.*",
        "dazzle-php/throwable": "0.5.*",
        "dazzle-php/util": "0.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.0 <5.4.0"
    },
    "suggest": {
        "ext-ssh2": "*"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\SSH\\": "src/Event",
            "Dazzle\\SSH\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
